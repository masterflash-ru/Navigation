{
    "name": "masterflash-ru/navigation",
    "description": "Генератор навигационных элементов разных стандартов",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash",
        "simba"
    ],
    "homepage": "https://www.masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "masterflash-ru/ado": "^2.2",
        "laminas/laminas-navigation": "^2.8"
    },
    "conflict": {
        "masterflash-ru/menu": "*"
    },
    "autoload": {
        "psr-4": {
            "Mf\\Navigation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Navigation\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.0-dev"
        },
        "zf": {
            "component": "Mf\\Navigation",
            "config-provider": "masterflash-ru\\Mf\\Navigation\\ConfigProvider"
        }
    }
}